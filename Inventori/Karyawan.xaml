<Window x:Class="inventory.Karyawan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:inventory"
        mc:Ignorable="d"
        Title="Karyawan" Height="450" Width="353">
    <Grid HorizontalAlignment="Left" Width="379">
        <TextBox x:Name="Nama" HorizontalAlignment="Left" Margin="37,103,0,0" Text="Nama " TextWrapping="Wrap" VerticalAlignment="Top" Width="192" Height="45" TextChanged="Nama_TextChanged"/>
        <TextBox x:Name="Telepon" HorizontalAlignment="Left" Margin="38,164,0,0" Text="No Telepon" TextWrapping="Wrap" VerticalAlignment="Top" Width="190" Height="44" TextChanged="Telepon_TextChanged"/>
        <TextBox x:Name="Sift" HorizontalAlignment="Left" Margin="39,223,0,0" Text="Sift Karyawan" TextWrapping="Wrap" VerticalAlignment="Top" Width="190" Height="44" TextChanged="Sift_TextChanged"/>
        <Button x:Name="Input" Content="Input" HorizontalAlignment="Left" Margin="40,285,0,0" VerticalAlignment="Top" Height="38" Width="190" Click="OnClickInput"/>
        <TextBox x:Name="KodeKaryawan" HorizontalAlignment="Left" Margin="37,44,0,0" Text="Kode Karyawan " TextWrapping="Wrap" VerticalAlignment="Top" Width="192" Height="45" TextChanged="KodeKaryawan_TextChanged"/>

    </Grid>
</Window>
