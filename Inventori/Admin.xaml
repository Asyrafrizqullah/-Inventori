<Window x:Class="inventory.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:inventory"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="290">
    <Grid HorizontalAlignment="Left" Width="403">
        <TextBox x:Name="kode" HorizontalAlignment="Left" Height="37" Margin="39,51,0,0" Text="Kode Admin" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" TextChanged="kode_TextChanged"/>
        <TextBox x:Name="Nama" HorizontalAlignment="Left" Height="37" Margin="40,104,0,0" Text="Nama" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" TextChanged="Nama_TextChanged"/>
        <TextBox x:Name="telepon" HorizontalAlignment="Left" Height="37" Margin="41,160,0,0" Text="No telepon" TextWrapping="Wrap" VerticalAlignment="Top" Width="150" TextChanged="telepon_TextChanged"/>
        <Button x:Name="Input" Content="Input" HorizontalAlignment="Left" Margin="42,218,0,0" VerticalAlignment="Top" Height="44" Width="150" Click="OnClickInput"/>
    </Grid>
</Window>
